/* pages/myFile.wxss */
.headImg{
  width: 100%;
  height: 200rpx;
  text-align: center;
  padding-top: 60rpx;
  color: gray;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.img{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.main{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 800rpx;
  margin-top: 35rpx;
}
.List{
  display: flex;
  justify-content: center;
  width: 80%;
  margin-left: 10%;
  margin-top: 30rpx;
  border-bottom: 1px solid #BDBDBE;
  border-top: 1px solid #BDBDBE;
}
.listImg{
  width: 40rpx;
  height: 40rpx;
  padding-top: 15rpx;
  cursor: pointer;
}
.list{
  font-size: 35rpx;
  color: gray;
  width: 500rpx;
  height: 25rpx;
}
.share{
  width: 60%;
  height: 50%;
  border: none;
  background-color: #FFFFFF;
}
